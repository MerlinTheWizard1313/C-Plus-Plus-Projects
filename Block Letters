#include <iostream>
//I hold a world first
int main(){
  std::cout << "M       M   B B B B  \n";
  std::cout << "M M   M M   B       B\n";
  std::cout << "M   M   M   B B B B  \n";
  std::cout << "M       M   B       B\n";
  std::cout << "M       M   B B B B  \n";
}

