#include <iostream>
int main() {

  double dog_age;
  double human_age;

  std::cout << "Enter your dog's age: \n";
  std::cin >> dog_age;

  if (dog_age <= 2)
  {
    human_age = dog_age * 10.5;
  }
  else
  {
    human_age = 21 + ((dog_age - 2) * 4);
  }

  std::cout << "Your dog's human age is " << human_age << " years old.\n";
}

compiler:
g++ dog_years.cpp
./a.out
